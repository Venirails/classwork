<h1>Admin Login</h1>

<%= form_for(:admin) do |f| %>

    <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :name %>
  </div>
  <br />
  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </div>
<br />
  <div class="actions">
    <%= f.submit "Login"%>
  </div>
<% end %>
